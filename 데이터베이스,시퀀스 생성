CREATE TABLE MESSAGE (
    IDX NUMBER PRIMARY KEY,
    NAME VARCHAR2(30),
    SUBJECT VARCHAR2(100),
    CONTENT VARCHAR2(4000),
    PWD VARCHAR2(50),
    REGDATE DATE 
    );
    
    
    
    SELECT * FROM FREE;
    
    
    
    CREATE SEQUENCE FREE_SEQ NOCACHE;
    
