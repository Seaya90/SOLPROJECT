CREATE TABLE MEMBER (
    IDX NUMBER PRIMARY KEY,
    ID VARCHAR2(20) NOT NULL,
    PWD VARCHAR2(20) NOT NULL,
    NAME VARCHAR(10) NOT NULL,
    GENDER VARCHAR(4),
    PHONE VARCHAR2(30),
    ADDRESS VARCHAR2(100),
    REGDATE DATE);
    
    INSERT INTO MEMBER 
     VALUES (MEMBER_SEQ.NEXTVAL,'USER', '0000' , 'user', 'W', '010-2929-8282', 'USA',SYSDATE);
    
    
CREATE SEQUENCE MEMBER_SEQ
    NOCACHE;
    
    
    SELECT * FROM member;
    
   
   
